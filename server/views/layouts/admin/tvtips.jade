h1 Admin: TV Tips

if tvTips
	each event in tvTips
		h2 
			a(href="/programme/#{event.programme.id}") #{event.programme.title}
		p Event id #{event.id} #{event.prettyStartTime}
			|on 
			a(href="/channel/#{event.channel.id}") #{event.channel.name}
		p #{event.programme.shortDescription}
		p
			img(src="#{event.programme.imageUrl}")
